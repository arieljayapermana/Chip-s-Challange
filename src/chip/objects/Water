package chip.objects;


/**
 *
 * @author Christofer Indra Sinarya
 */
public class Water extends Tile{

    public Water() {
        this.img = tk.getImage(this.getClass().getResource("images\\water.png"));
    }

    @Override
    public boolean isMoveable() {
        return true;
    }

    @Override
    public boolean isLethal() {
        return true;
    }
    
}
